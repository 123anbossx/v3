    // "noLib": true,                                    /* 禁止包含任何库文件，包括默认的 lib.d.ts */
    // "useDefineForClassFields": true,                  /* 发出符合 ECMAScript 标准的类字段 */
    // "moduleDetection": "auto",                        /* 控制用于检测模块格式 JS 文件的方法 */

    /* 模块 */
    "module": "commonjs",                                /* 指定生成的模块代码 */
