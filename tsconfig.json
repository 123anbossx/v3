    // "emitDecoratorMetadata": true,                    /* 为源文件中的装饰声明发出设计类型元数据 */
    // "jsxFactory": "",                                 /* 指定用于目标 React JSX 生成的 JSX 工厂函数，例如 'React.createElement' 或 'h' */
    // "jsxFragmentFactory": "",                         /* 指定用于片段的 JSX Fragment 引用，例如 'React.Fragment' 或 'Fragment' */
